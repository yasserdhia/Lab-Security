{
  "name": "sql-injection-lab",
  "version": "1.0.0",
  "description": "A comprehensive SQL injection testing lab with multiple vulnerability levels",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev-down": "docker-compose -f docker-compose.dev.yml down",
    "db:init": "docker exec -i sqli-lab-db psql -U postgres -d sqli_lab < database/init.sql",
    "db:setup": "docker run -d --name sqli-lab-db -e POSTGRES_DB=sqli_lab -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=password -p 5432:5432 postgres:15",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "pg": "^8.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/pg": "^8.11.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/cookie": "^0.6.0",
    "typescript": "^5.4.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@types/eslint": "^8.56.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0"
  },
  "keywords": [
    "sql-injection",
    "security",
    "penetration-testing",
    "nextjs",
    "postgresql",
    "cybersecurity"
  ]
}
